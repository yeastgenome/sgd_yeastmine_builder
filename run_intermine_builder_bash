docker run -it -e MINE_NAME=yeastmine \
	--net build \
	-v intermine_data:/root/data \
	-e MINE_NAME=yeastmine \
	-e MINE_REPO_URL=https://github.com/yeastgenome/yeastmine.git \
        -e BIOSOURCES_REPO_URL=https://github.com/yeastgenome/yeastmine-bio-sources.git \
	-e INTERMINE_PGHOST=yeastmine-db.dev.yeastgenome.org \
	-e SOLR_HOST=yeastmine-docker-2a.dev.yeastgenome.org \
	-e TOMCAT_HOST=yeastmine-docker-2a.dev.yeastgenome.org intermine_builder bash
